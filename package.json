{
  "name": "kimjansheden-logo",
  "version": "1.1.1",
  "description": "A React component for displaying Kim Jansheden's branded logo",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "style": "dist/styles.css",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "styles.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:link": "npm run build && npm link",
    "deploy": "npm run build && npm publish --access public",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:visual": "vitest run src/tests/KimJanshedenLogo.visual.test.tsx",
    "test:hover": "vitest run src/tests/KimJanshedenLogo.visual.test.tsx --reporter=verbose"
  },
  "keywords": [
    "react",
    "logo",
    "branding",
    "tailwind",
    "kimjansheden"
  ],
  "author": {
    "name": "Kim Jansheden",
    "email": "kim.jansheden@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimjansheden/kimjansheden-logo.git"
  },
  "homepage": "https://kimjansheden.se",
  "bugs": {
    "url": "https://github.com/kimjansheden/kimjansheden-logo/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "postcss": "^8.5.3",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tailwindcss": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@tailwindcss/postcss": {
      "optional": true
    },
    "postcss": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.21",
    "@types/react": ">=19.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.7",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
